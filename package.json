{
  "name": "agency-pkg-element-light-spot",
  "version": "0.0.1",
  "description": "agency - frontend-package",
  "keywords": [
    "boilerplate",
    "package"
  ],
  "homepage": "https://github.com/agency-framework/agency-pkg-element-light-spot#readme",
  "bugs": {
    "url": "https://github.com/agency-framework/agency-pkg-element-light-spot/issues"
  },
  "license": "MIT",
  "author": "Thorn Walli <lammpee@gmail.com>",
  "contributors": [
    {
      "name": "Thorn Walli",
      "email": "lammpee@gmail.com; thorn.walli@grabarzundpartner.de"
    }
  ],
  "main": "default.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agency-framework/agency-pkg-element-light-spot.git"
  },
  "scripts": {
    "build": "node node_modules/gulp/bin/gulp build --serverConfig=node_modules/agency-boilerplate/env/config/agency/server.json --configTasks=./env/config/agency/tasks.json --env=package-build",
    "install": "node ./install.js",
    "heroku": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 node_modules/gulp/bin/gulp run --serverConfig=node_modules/agency-boilerplate/env/config/agency/server.json --configTasks=./env/config/agency/tasks.json --env=package-production --server=agency-server --heroku=true",
    "dev": "node node_modules/gulp/bin/gulp run --serverConfig=node_modules/agency-boilerplate/env/config/agency/server.json --configTasks=env/config/agency/tasks.json --env=package-development --server=agency-server",
    "prod": "node node_modules/gulp/bin/gulp run --serverConfig=node_modules/agency-boilerplate/env/config/agency/server.json --configTasks=env/config/agency/tasks.json --env=package-production --server=agency-server"
  },
  "devDependencies": {
    "agency-boilerplate": "git+https://git@github.com/agency-framework/agency-boilerplate.git"
  },
  "engines": {
    "node": "7"
  },
  "dependencies": {
    "iphone-inline-video": "^1.9.6",
    "screenfull": "^3.0.2"
  }
}
